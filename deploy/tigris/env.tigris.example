# Canonical S3 runtime settings (Tigris-compatible)

# Required
export BUCKET_NAME=crdtbase
export AWS_ENDPOINT_URL_S3=https://<your-tigris-s3-endpoint>
export AWS_ACCESS_KEY_ID=<your-access-key-id>
export AWS_SECRET_ACCESS_KEY=<your-secret-access-key>
export AWS_REGION=auto

# Optional for this repo's REPL defaults
export SITE_ID=site-a
export S3_PREFIX=deltas
export AWS_SESSION_TOKEN=
export S3_FORCE_PATH_STYLE=false
